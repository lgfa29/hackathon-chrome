{
  "manifest_version": 2,
  "name": "James",
  "options_page": "html/options.html",
  "version": "0.0.1",
  "permissions": ["notifications", "storage"],
  "browser_action": {
    "default_icon": {
      "19": "img/icon_enabled.png"
    }
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/lib/guid.js",
      "js/background.js"
    ]
  },
  "minimum_chrome_version": "23"
}